
<h4>{{golfer.name}}</h4>
<ul>
	<li><strong>Ranking:</strong> {{golfer.ranking}}</li>
	<li><strong>Country:</strong> {{golfer.country}}</li>
</ul>

<table class="table">
<thead>
	<tr>
		<th>Round 1</th>
		<th>Round 2</th>
		<th>Round 3</th>
		<th>Round 4</th>
		<th>Total</th>
	</tr>
</thead>
<tbody>
	<tr>
		<td>{{golfer.r1}}</td>
		<td>{{golfer.r2}}</td>
		<td>{{golfer.r3}}</td>
		<td>{{golfer.r4}}</td>
		<td>{{golfer.score}}</td>
	</tr>
</tbody>
</table>
<hr>

<h5><strong>People who picked him up:</strong></h5>
{{#each golfer.teams as |team|}}
	<h6>{{team.user.name}}</h6>
{{/each}}